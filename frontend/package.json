    "install": "^0.10.1",
    "jest": "20.0.4",
    "json2csv": "^3.11.4",
    "konva": "^1.7.6",
    "lodash": "^4.17.4",
    "material-ui": "^0.19.4",
    "material-ui-datatables": "^0.18.2",
